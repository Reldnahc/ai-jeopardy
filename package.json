{
  "name": "ai-jeopardy",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://ai-jeopardy.com/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite --host 0.0.0.0 --port 5173",
    "dev:backend": "tsx backend/server.ts",
    "build:web": "vite build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build": "npm run build:web && npm run build:backend",
    "start": "node dist-backend/backend/server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build:web",
    "deploy": "gh-pages -d dist --cname ai-jeopardy.com",
    "dev:docker": "docker compose -f docker-compose.dev.yml up",
    "dev:docker:detached": "docker compose -f docker-compose.dev.yml up -d",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml down",
    "dev:docker:logs": "docker compose -f docker-compose.dev.yml logs -f"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@tanstack/react-query": "^5.64.2",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "framer-motion": "^12.1.0",
    "jsonwebtoken": "^9.0.3",
    "openai": "^4.80.0",
    "pg": "^8.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.3",
    "react-sketch-canvas": "^6.2.0",
    "sharp": "^0.34.5",
    "undici": "^7.20.0",
    "ws": "^8.18.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.2",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
